@import '../styles/iconfont.wxss';
.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 98rpx;
  background: white;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
}
.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.tab-bar-item cover-view {
  font-size: 28rpx;
  color: #333333;
}
.tab-bar-item .icomoon{
  font-size: 44rpx; 
}